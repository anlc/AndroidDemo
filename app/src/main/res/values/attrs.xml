<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TabLayout">
        <attr name="line_indicate_color" format="color" />
        <attr name="line_indicate_drawable" format="reference" />
        <attr name="tab_title_color" format="color|reference" />
        <attr name="line_indicate_width" format="enum">
            <enum name="wrap_content" value="0" />
            <enum name="match_parent" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MultiSquareEditView">
        <attr name="square_count" format="integer" />
        <attr name="item_margin" format="dimension" />
        <attr name="item_width" format="dimension" />
        <attr name="item_height" format="dimension" />
        <attr name="square_bg" format="color" />
    </declare-styleable>

    <attr name="item_key" format="string" />
    <attr name="item_key_color" format="reference|color" />
    <attr name="item_key_size" format="integer" />
    <attr name="item_value" format="string" />
    <attr name="item_hint_value" format="string" />
    <attr name="item_value_color" format="reference|color" />
    <attr name="item_hint_value_color" format="reference|color" />
    <attr name="item_value_size" format="integer" />
    <attr name="item_show_arrow" format="reference|boolean" />
    <attr name="item_show_bottom_line" format="reference|boolean" />
    <attr name="item_show_add_img" format="reference|boolean" />
    <attr name="item_show_bottom_dash" format="boolean" />

    <declare-styleable name="ListItemView">
        <attr name="item_key" />
        <attr name="item_key_color" />
        <attr name="item_key_size" />
        <attr name="item_value" />
        <attr name="item_value_color" />
        <attr name="item_value_size" />
        <attr name="item_show_arrow" />
        <attr name="item_show_bottom_line" />
        <attr name="item_right_img" format="reference" />
        <attr name="item_show_bottom_dash" />
    </declare-styleable>

    <declare-styleable name="SelectItemView">
        <attr name="item_text_color" format="reference|color" />
        <attr name="item_text_size" format="dimension" />
        <attr name="select_text_color" format="reference|color" />
        <attr name="select_text_size" format="dimension" />
        <attr name="item_padding" format="dimension" />
        <attr name="android:orientation" />
    </declare-styleable>

    <declare-styleable name="TitleSeekBar">
        <attr name="title" />
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
    </declare-styleable>
</resources>